# TWRP flags for rtwo made by GitFASTBOOT
# mount point      fstype       device                                     device2       flags

# AB partitions
/system_a           emmc        /dev/block/mapper/system_a                                flags=backup=1;flashimg=1;slotselect;display="System Image A"
/vendor_a           emmc        /dev/block/mapper/vendor_a                                flags=backup=1;flashimg=1;slotselect;display="Vendor Image A"
/product_a          emmc        /dev/block/mapper/product_a                               flags=backup=1;flashimg=1;slotselect;display="Product Image A"
