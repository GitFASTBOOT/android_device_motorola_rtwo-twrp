/system_dlkm_a      emmc        /dev/block/mapper/system_dlkm_a                           flags=backup=1;flashimg=1;slotselect;display="System_DLKM Image A"
/boot_a             emmc        /dev/block/by-name/boot_a                                 flags=backup=1;flashimg=1;slotselect;display="Boot_a"
